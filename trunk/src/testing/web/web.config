<?xml version="1.0"?>
<configuration xmlns="http://schemas.microsoft.com/.NetConfiguration/v2.0">
  <connectionStrings>
    <add name="SiteSqlServer" connectionString="Data Source='PLANANALISTA';Initial Catalog='Hydra';User ID='accao_pass';Password='pass(accao)';Asynchronous Processing=true;" />
  </connectionStrings>
  
  <appSettings>
    <add key="NohrosConfigurationFile" value="nohros.config" />
  </appSettings>
  
    <!-- Public Area -->
  <system.web>
    <httpModules></httpModules>

    <httpHandlers>
        <add path="*.nohrosnet.*" verb="GET" type="Nohros.Net.MergeHttpHandler, Nohros.Net"/>
    </httpHandlers>

    <compilation debug="true" defaultLanguage="c#">
    </compilation>
    <customErrors mode="Off">
      <error statusCode="404" redirect="/404.aspx" />
    </customErrors>
    <authentication mode="Forms">
      <forms name="HSignIn" loginUrl="Login.aspx" protection="All" timeout="60">
      </forms>
    </authentication>
    <authorization>
      <allow users="*"/>
    </authorization>
    <sessionState mode="InProc" timeout="20"/>
    <globalization requestEncoding="utf-8" responseEncoding="utf-8"/>
  </system.web>
  <!-- End Public Area -->
</configuration>